[Desktop Entry]
Name=@ThemeName@ on Wayland
Comment=This session logs you with @ThemeName@ on wayland
Exec=env GNOME_SHELL_SESSION_MODE=@LowerCaseThemeName@ /usr/bin/gnome-session --systemd --session=ubuntu
TryExec=/usr/bin/gnome-session
Type=Application
DesktopNames=@ThemeName@:ubuntu:GNOME
X-Ubuntu-Gettext-Domain=gnome-session-3.0
